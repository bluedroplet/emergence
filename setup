#!/bin/bash

ln -sfn stage3-pentium3-2004.1 ../stage3s/current-stage3
export CFLAGS="-march=pentium3 -O2 -pipe -g -fomit-frame-pointer -fprefetch-loop-arrays"


cd em-game

make distclean

aclocal-1.8
automake-1.8
autoconf-2.59
./configure

cd ../em-tools

make distclean

aclocal-1.8
automake-1.8
autoconf-2.59
./configure

cd ../misc

make distclean

aclocal-1.8
automake-1.8
autoconf-2.59
./configure

cd ../
